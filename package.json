{
  "name": "innermind.boilerplate",
  "version": "1.0.0",
  "description": "TypeScript Project Boilerplate",
  "license": "MIT",
  "author": "Jason Breitigan",
  "homepage": "https://github.com/JBreit/innermind.boilerplate#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JBreit/innermind.boilerplate.git"
  },
  "bugs": {
    "url": "https://github.com/JBreit/innermind.boilerplate/issues"
  },
  "main": "./src/main.ts",
  "directories": {
    "doc": "docs",
    "test": "tests",
    "tools": "tools"
  },
  "scripts": {
    "browserslist": "npx browserslist",
    "browserslist:lint": "npx browserslist-lint",
    "build": "tsc",
    "build:babel": "npx babel --config-file ./babel.config.cjs --extensions \".ts\" ./src/main.ts -d ./dist",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "clean": "rimraf ./dist",
    "commit": "git-cz",
    "kill": "taskkill /F /IM node.exe",
    "lint": "npx eslint --ignore-path .eslintignore . --cache --max-warnings=0 --ext .ts,.tsx",
    "lint:fix": "npx eslint --ignore-path .eslintignore . --cache --max-warnings=0 --ext .ts,.tsx --fix",
    "mdlint": "markdownlint .",
    "mdlint:fix": "npm run mdlint --fix",
    "prepare": "npx husky init",
    "prettier:format": "npx prettier --config .prettierrc.cjs ./src/**/*.ts --write",
    "prettier:package": "npx prettier-package-json --write",
    "prettier:watch": "npx onchange ./src/**/*.ts -- prettier --write {{changed}}",
    "prestart": "npm install",
    "start": "npm run build && cross-env NODE_ENV=production node ./dist/main.js",
    "start:dev": "npx nodemon",
    "test": "npx jest --verbose",
    "test:coverage": "rimraf ./coverage && npx jest --verbose --coverage",
    "test:watch": "jest --verbose --watchAll -i",
    "update": "ncu --doctor -u",
    "preversion": "npm run test",
    "version": "conventional-changelog -p angular -i CHANGELOG.md -s && git add CHANGELOG.md"
  },
  "config": {
    "commitizen": {
      "path": "./tools/cz.adapter.cjs",
      "useEmoji": true
    }
  },
  "peerDependencies": {
    "eslint": "^8.57.0",
    "react": "^18.2.0",
    "typescript": "^5.3.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.23.9",
    "@babel/core": "^7.24.0",
    "@babel/eslint-parser": "^7.23.10",
    "@babel/plugin-proposal-async-do-expressions": "^7.23.3",
    "@babel/plugin-proposal-decorators": "^7.24.0",
    "@babel/plugin-proposal-destructuring-private": "^7.23.3",
    "@babel/plugin-proposal-do-expressions": "^7.23.3",
    "@babel/plugin-proposal-duplicate-named-capturing-groups-regex": "^7.23.3",
    "@babel/plugin-proposal-explicit-resource-management": "^7.23.9",
    "@babel/plugin-proposal-export-default-from": "^7.23.3",
    "@babel/plugin-proposal-function-bind": "^7.23.3",
    "@babel/plugin-proposal-function-sent": "^7.23.3",
    "@babel/plugin-proposal-import-attributes-to-assertions": "^7.23.3",
    "@babel/plugin-proposal-import-defer": "^7.23.7",
    "@babel/plugin-proposal-partial-application": "^7.23.3",
    "@babel/plugin-proposal-pipeline-operator": "^7.23.3",
    "@babel/plugin-proposal-record-and-tuple": "^7.23.3",
    "@babel/plugin-proposal-regexp-modifiers": "^7.23.3",
    "@babel/plugin-proposal-throw-expressions": "^7.23.3",
    "@babel/plugin-syntax-import-assertions": "^7.23.3",
    "@babel/plugin-syntax-import-attributes": "^7.23.3",
    "@babel/preset-env": "^7.24.0",
    "@babel/preset-react": "^7.23.3",
    "@babel/preset-typescript": "^7.23.3",
    "@babel/register": "^7.23.7",
    "@babel/runtime": "^7.24.0",
    "@babel/runtime-corejs3": "^7.24.0",
    "@bloomberg/record-tuple-polyfill": "^0.0.4",
    "@commitlint/cli": "^19.0.3",
    "@commitlint/config-conventional": "^19.0.3",
    "@commitlint/cz-commitlint": "^19.0.3",
    "@jest/globals": "^29.7.0",
    "@lhci/cli": "^0.13.0",
    "@stylistic/eslint-plugin": "^1.6.3",
    "@stylistic/eslint-plugin-js": "^1.6.3",
    "@stylistic/eslint-plugin-ts": "^1.6.3",
    "@tsconfig/node21": "^21.0.1",
    "@types/core-js": "^2.5.8",
    "@types/faker": "^6.6.9",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.24",
    "@types/shelljs": "^0.8.15",
    "@typescript-eslint/eslint-plugin": "^7.1.0",
    "@typescript-eslint/parser": "^7.1.0",
    "babel-jest": "^29.7.0",
    "babel-plugin-module-resolver": "^5.0.0",
    "babel-plugin-react-html-attrs": "^3.0.5",
    "commitizen": "^4.3.0",
    "conventional-changelog": "^5.1.0",
    "conventional-changelog-cli": "^4.1.0",
    "cross-env": "^7.0.3",
    "cz-git": "^1.9.0",
    "eslint": "^8.57.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jest": "^27.9.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-watch": "^8.0.0",
    "faker": "^5.5.3",
    "git-commit-msg-linter": "^5.0.6",
    "git-rev-sync": "^3.0.2",
    "glob": "^10.3.10",
    "glob-all": "^3.3.1",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.2",
    "markdownlint": "^0.33.0",
    "onchange": "^7.1.0",
    "prettier": "^3.2.5",
    "regenerator": "^0.14.12",
    "rimraf": "^5.0.5",
    "semver": "^7.6.0",
    "shelljs": "^0.8.5",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "tsconfig-paths-jest": "^0.0.1",
    "typedoc": "^0.25.9",
    "typescript": "^5.3.3",
    "validate-branch-name": "^1.3.0"
  },
  "keywords": [
    "airbnb",
    "async",
    "await",
    "babel",
    "boilerplate",
    "browserslist",
    "commitizen",
    "commitlint",
    "conventional-changelog",
    "cross-env",
    "cz-git",
    "dotenv",
    "editorconfig",
    "eslint",
    "git",
    "husky",
    "jest",
    "lint-staged",
    "markdownlint",
    "node",
    "nodemon",
    "prettier",
    "semver",
    "shelljs",
    "stylistic",
    "ts-node",
    "tsconfig-paths",
    "typescript"
  ],
  "engines": {
    "node": ">=21.1.0",
    "npm": ">=10.5.0"
  }
}
